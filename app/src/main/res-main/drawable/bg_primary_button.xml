<?xml version="1.0" encoding="utf-8"?>
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetBottom="@dimen/abc_button_inset_vertical"
    android:insetLeft="@dimen/abc_button_inset_horizontal"
    android:insetRight="@dimen/abc_button_inset_horizontal"
    android:insetTop="@dimen/abc_button_inset_vertical">
    <shape android:shape="rectangle">
        <corners android:radius="25dp" />
        <solid android:color="@color/colorAccent" />
        <padding
            android:bottom="@dimen/abc_button_padding_horizontal"
            android:left="@dimen/abc_button_padding_vertical"
            android:right="@dimen/abc_button_padding_vertical"
            android:top="@dimen/abc_button_padding_horizontal" />
    </shape>
</inset>